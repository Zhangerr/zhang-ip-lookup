{
  "name": "buycpanel utils",
  "version": "1",
  "manifest_version": 2,
  "description": "handy little things, hooks into whmcs billing",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons":
  { 
    "48" : "icon48.png"
  },
   "content_scripts": [
    {
      "matches": ["http://billing.buycpanel.com/*","https://billing.buycpanel.com/*","http://orders.buycpanel.com/*"],      
      "js": ["jquery-1.9.1.js", "bg.js"]
    }
  ],
  "web_accessible_resources": [
  ]
}
